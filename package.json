{
  "name": "utcc-web",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start:prod": "ng serve --prod --watch --configuration=production",
    "start:dev-mock": "concurrently \"cd src/mocks && npm i --force && npm run start:dev\" \"ng serve --configuration=dev-mock\"",
    "build:sit": "ng build --build-optimizer --configuration=sit --base-href=/utcc-web-sit/ --aot --output-hashing=all --output-path=dist/utcc-web-sit/",
    "build:uat": "ng build --build-optimizer --configuration=uat --base-href=/utcc-web-uat/ --aot --output-hashing=all --output-path=dist/utcc-web-uat/",
    "build:prod": "ng build --build-optimizer --configuration=production --base-href=/utcc-web/ --aot --output-hashing=all",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "dev": "ng serve --host 0.0.0.0"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.1",
    "@angular/common": "^16.2.1",
    "@angular/compiler": "^16.2.1",
    "@angular/core": "^16.2.1",
    "@angular/forms": "^16.2.1",
    "@angular/platform-browser": "^16.2.1",
    "@angular/platform-browser-dynamic": "^16.2.1",
    "@angular/router": "^16.2.1",
    "@googlemaps/js-api-loader": "^1.16.2",
    "chart.js": "^4.4.0",
    "moment": "^2.29.4",
    "primeflex": "^3.3.1",
    "primeicons": "^6.0.1",
    "primeng": "^16.2.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.0",
    "@angular/cli": "~16.2.0",
    "@angular/compiler-cli": "^16.2.1",
    "@types/google.maps": "^3.54.1",
    "@types/jasmine": "~4.3.0",
    "force": "^0.0.3",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "typescript": "~4.9.4"
  }
}